<Window x:Class="FBLeagueTimer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FBLeagueTimer"
        mc:Ignorable="d"
        Title="MainWindow" Height="315" Width="315.5">
    <Grid>
        <Button Content="FirstBlood" HorizontalAlignment="Left" Margin="210,11,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Matches" HorizontalAlignment="Left" Margin="210,37,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <TextBox Name="LoggingTextBox" HorizontalAlignment="Left" Height="191" Margin="19,73,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="266"/>
        <Label Content="Name:" HorizontalAlignment="Left" Margin="19,8,0,0" VerticalAlignment="Top"/>
        <Label Content="First bloods:" HorizontalAlignment="Left" Margin="19,34,0,0" VerticalAlignment="Top"/>
        <Label Name="FirstBloodCount" Content="0" HorizontalAlignment="Left" Margin="98,34,0,0" VerticalAlignment="Top" Width="37" RenderTransformOrigin="0.432,1.231"/>
        <TextBox Name="PlayerName" HorizontalAlignment="Left" Height="23" Margin="68,11,0,0" TextWrapping="Wrap" Text="QLerin" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
